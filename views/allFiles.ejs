<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>Jupiter</title>

    <script src="../js/jquery.min.js"></script>
    <script src="../js/jquery.waypoints.min.js"></script>
    <script src="../js/jquery.stellar.min.js"></script>
    <script src="../js/hoverIntent.js"></script>
    <script src="../js/superfish.js"></script>
    <link rel="stylesheet" href="../css/bootstrap.css">
    <script src="../js/bootstrap.min.js"></script>
    <script src="../js/jquery.easing.1.3.js"></script>
    <script src="../js/main.js"></script>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>

<body>
    <div class="container text-center">
        <div class="col-md-12">
            <table class="table">
                <tbody>
                    <% for (file of fileList) { %>
                    <tr>
                        <td>
                            <i class="material-icons filIcon" style="font-size:35px; color:#1175da">insert_drive_file</i>
                        </td>
                        <td>
                            <a href="/download/<%= file.name %>">
                                <h4><%= file.name %></h4>
                            </a>
                        </td>
                        <td>
                            <button id="editButton" class="btn btn-warning btn-sm" onclick="redirect('./edit/<%= file._id %>')">Edit</button>
                        </td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>

    <script>
        function redirect(url) {
            location.href = (url);
        }
    </script>
</body>

</html>